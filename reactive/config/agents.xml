<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### REACTIVE AGENTS ### -->
	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="reactive.RandomBehavior"/>
		<set discount-factor=".85"/>
	</agent>

	<agent name="reactive-rla">
		<set class-path="bin/"/>
		<set class-name="reactive.RLABehavior"/>
		<set discount-factor="0.95"/>
	</agent>

	<agent name="reactive-greedy">
		<set class-path="bin/"/>
		<set class-name="reactive.GreedyBehavior"/>
	</agent>
	
	<!--  <agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="template.RandomBehavior"/>
		<set discount-factor=".85"/>
	</agent> -->
</agents>
